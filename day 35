#include <stdio.h>

int main() {
    int T, X, A, B;
    
   
    scanf("%d", &T);
    
    
    for (int i = 0; i < T; i++) {
        
        scanf("%d %d %d", &X, &A, &B);
        
        
        int total_score = A + (2 * B);
        
       
        if (total_score >= X) {
            printf("Qualify\n");
        } else {
            printf("NotQualify\n");
        }
    }
    
    return 0;
}
