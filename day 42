#include <stdio.h>

int main() {
    int T;  
    scanf("%d", &T);
    
    while (T--) {
        int A, B, C;
        scanf("%d %d %d", &A, &B, &C);
        
        
        int total_price = (A + B + C) - (A < B ? (A < C ? A : C) : (B < C ? B : C));
        
       
        printf("%d\n", total_price);
    }

    return 0;
}
