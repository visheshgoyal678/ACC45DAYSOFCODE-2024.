#include <stdio.h>

int main() {
    int T; // Number of test cases
    scanf("%d", &T);
    
    while (T--) {
        int N; // Number of spooky days
        scanf("%d", &N);
        
        // Calculate the number of Tuesdays
        int tuesdays_count = 1 + (N - 1) / 7;
        
        // Output the result
        printf("%d\n", tuesdays_count);
    }

    return 0;
}
