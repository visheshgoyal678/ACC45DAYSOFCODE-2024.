#include <stdio.h>
#include <stdlib.h>

int main() {
    int T;
    scanf("%d", &T);
    
    while (T--) {
        int A, B, K;
        scanf("%d %d %d", &A, &B, &K);
        
    
        int distance = abs(A - B);
        
      
        int steps = (distance + K - 1) / K; // Equivalent to ceiling(distance / K)
        
        
        printf("%d\n", steps);
    }
    
    return 0;
}
